    "version": "0.2.0",
    "configurations": [
        
        {
            "type": "java",
            "name": "Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "IdentityStreamingScript",
            "request": "launch",
            "mainClass": "org.apache.hyracks.algebricks.tests.script.IdentityStreamingScript",
            "projectName": "algebricks-tests"
        },
        {
            "type": "java",
            "name": "AsterixHelper",
            "request": "launch",
            "mainClass": "org.apache.asterix.clienthelper.AsterixHelper",
            "projectName": "asterix-client-helper"
        },
        {
            "type": "java",
            "name": "FuzzyJoinMemory",
            "request": "launch",
            "mainClass": "org.apache.asterix.fuzzyjoin.FuzzyJoinMemory",
            "projectName": "asterix-fuzzyjoin"
        },
        {
            "type": "java",
            "name": "CCDriver",
            "request": "launch",
            "mainClass": "org.apache.hyracks.control.cc.CCDriver",
            "projectName": "hyracks-control-cc"
        },
        {
            "type": "java",
            "name": "NCDriver",
            "request": "launch",
            "mainClass": "org.apache.hyracks.control.nc.NCDriver",
            "projectName": "hyracks-control-nc"
        },
        {
            "type": "java",
            "name": "NCService",
            "request": "launch",
            "mainClass": "org.apache.hyracks.control.nc.service.NCService",
            "projectName": "hyracks-nc-service"
        },
        {
            "type": "java",
            "name": "LexerGenerator",
