            "vmArgs": "-Dcleanup.start=true -Xms6144m -Xmx6144m -Dnode.Resolver=\"org.apache.asterix.external.util.IdentityResolverFactory\"",
            "args": "",
            "console": "integratedTerminal"
        },
        {
            "type": "java",
            "name": "AsterixHyracksIntegrationUtil-Maven",
            "request": "launch",
            "mainClass": "org.apache.asterix.test.server.AsterixServerIntegrationUtil",
            "vmArgs": "-Dcleanup.start=true -Xms6144m -Xmx6144m -Dnode.Resolver=\"org.apache.asterix.external.util.IdentityResolverFactory\"",
            "args": "",
            "console": "integratedTerminal",
            "preLaunchTask": "maven-compile"
        }
