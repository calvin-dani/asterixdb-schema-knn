        {
            "type": "java",
            "name": "WordCountMain",
            "request": "launch",
            "mainClass": "org.apache.hyracks.examples.text.client.WordCountMain",
            "projectName": "textclient"
        },
        {
            "type": "java",
            "name": "Groupby",
            "request": "launch",
            "mainClass": "org.apache.hyracks.examples.tpch.client.Groupby",
            "projectName": "tpchclient"
        },
        {
            "type": "java",
            "name": "Join",
            "request": "launch",
            "mainClass": "org.apache.hyracks.examples.tpch.client.Join",
            "projectName": "tpchclient"
