<test-suite
        xmlns="urn:xml.testframework.asterix.apache.org"
        ResultOffsetPath="results"
        QueryOffsetPath="queries_sqlpp"
        QueryFileExtension=".query.sqlpp">
    <test-group name="vector-distance">
        <test-case FilePath="vector-distance">
            <compilation-unit name="test-case-cosine">
                <output-dir compare="Clean-JSON">cosine</output-dir>
            </compilation-unit>
        </test-case>
        <test-case FilePath="vector-distance">
            <compilation-unit name="test-case-crossproduct">
                <output-dir compare="Clean-JSON">crossproduct</output-dir>
            </compilation-unit>
        </test-case>
        <test-case FilePath="vector-distance">
            <compilation-unit name="test-case-dotproduct">
                <output-dir compare="Clean-JSON">dotproduct</output-dir>
            </compilation-unit>
        </test-case>
        <test-case FilePath="vector-distance">
            <compilation-unit name="test-case-euclidean-lowercase">
                <output-dir compare="Clean-JSON">euclidean-lowercase</output-dir>
            </compilation-unit>
        </test-case>
        <test-case FilePath="vector-distance">
            <compilation-unit name="test-case-euclidean-uppercase">
                <output-dir compare="Clean-JSON">euclidean-uppercase</output-dir>
            </compilation-unit>
        </test-case>
        <test-case FilePath="vector-distance">
            <compilation-unit name="test-case-invaliddimensions">
                <output-dir compare="Clean-JSON">invaliddimensions</output-dir>
            </compilation-unit>
        </test-case>
    </test-group>
</test-suite>